<h1>Listing dashboards</h1>

<table class="table table-hover">
  <thead>
  <tr>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
  </thead>

  <tbody>
  <% @dashboards.each do |dashboard| %>
      <tr>
        <td><%= dashboard.title %></td>
        <td><span class="glyphicon glyphicon-eye-open"></span><%= link_to 'Show', dashboard %></td>
        <td><span class="glyphicon glyphicon-pencil"></span><%= link_to 'Edit', edit_dashboard_path(dashboard) %></td>
        <td><span class="glyphicon glyphicon-trash"></span><%= link_to 'Destroy', dashboard, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
  <% end %>
  </tbody>
</table>


<br>

<%= link_to 'New Dashboard', new_dashboard_path %>
